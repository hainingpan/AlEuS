	Doi = {10.1038/s41598-017-03415-3},
	Id = {Degtyarev2017},
	Isbn = {2045-2322},
	Journal = {Scientific Reports},
	Number = {1},
	Pages = {3411},
	Title = {Features of electron gas in {I}n{A}s nanowires imposed by interplay between nanowire geometry, doping and surface states},
	Ty = {JOUR},
	Url = {https://doi.org/10.1038/s41598-017-03415-3},
